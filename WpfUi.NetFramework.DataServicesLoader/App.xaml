<Application x:Class="WpfUi.NetFramework.DataServicesLoader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:s="https://github.com/canton7/Stylet"
             xmlns:dataServicesLoader="clr-namespace:WpfUi.NetFramework.DataServicesLoader">
  <Application.Resources>
    <s:ApplicationLoader>
      <s:ApplicationLoader.Bootstrapper>
        <dataServicesLoader:Bootstrapper/>
      </s:ApplicationLoader.Bootstrapper>
    </s:ApplicationLoader>
  </Application.Resources>
</Application>
